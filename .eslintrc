{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "shallow": true,
    "mount": true
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "react/prop-types": [0]
  }
}